            "expr": "SELECT \"sensor_name\", last(\"temperature\"), last(\"humidity\"), last(\"pressure\"), last(\"battery_voltage\"), last(\"rssi\") FROM \"ruuvi_measurements\" WHERE $timeFilter GROUP BY \"sensor_name\"",
